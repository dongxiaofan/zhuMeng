.examine-wrap {
  padding: 10px;
}
.examine-wrap .examine-title-wrap {
  margin-bottom: 10px;
}
.examine-wrap .examine-title-wrap .examine-title {
  font-size: 14px;
  color: #333;
  margin-left: 5px;
}
.examine-block {
  letter-spacing: 1px;
  background-color: #f2f6f9;
  padding: 10px;
  border-radius: 5px;
  margin-bottom: 18px;
}
.examine-title-mulu-list {
  padding: 10px;
  box-sizing: border-box;
}
.examine-title-mulu-list .mulu-item {
  height: 20px;
  line-height: 20px;
  display: flex;
  position: relative;
  margin-bottom: 10px;
  border-bottom: dotted 1px #ebebeb;
  padding-bottom: 10px;
}
.examine-title-mulu-list .mulu-item:last-child {
  border-bottom: 0;
}
.examine-title-mulu-list .mulu-item .iconfont {
  width: 8%;
  display: inline-block;
}
.examine-title-mulu-list .mulu-item .mulu-item-txt {
  width: 70%;
  font-size: 12px;
  color: #666;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  padding-right: 10px;
}
.examine-title-mulu-list .mulu-item .mulu-item-status {
  width: 20%;
  display: inline-block;
  font-size: 12px;
  text-align: right;
}
.text-primary {
  color: #007aff;
}
.text-grey {
  color: #999;
}